{
  "name": "bulma-jsx",
  "version": "0.2.4",
  "description": "A set of React Components for Bulma CSS",
  "main": "lib/index.js",
  "types": "lib/index.d.ts",
  "jsnext:main": "lib-esm/index.js",
  "module": "lib-esm/index.js",
  "dependencies": {
    "classnames": "^2.2.5",
    "tslib": "^1.7.0"
  },
  "devDependencies": {
    "@types/classnames": "^0.0.32",
    "@types/enzyme": "^2.8.0",
    "@types/jest": "^19.2.3",
    "@types/node": "^7.0.15",
    "@types/react": "^15.0.25",
    "@types/react-dom": "^15.5.0",
    "awesome-typescript-loader": "^3.1.3",
    "bulma": "^0.4.1",
    "coveralls": "^2.13.1",
    "css-loader": "^0.28.1",
    "enzyme": "^2.8.2",
    "jest": "^20.0.3",
    "react": "^15.5.4",
    "react-dom": "^15.5.4",
    "react-test-renderer": "^15.5.4",
    "shx": "^0.2.2",
    "style-loader": "^0.17.0",
    "typescript": "next",
    "webpack": "^2.4.1",
    "webpack-dev-server": "^2.4.5"
  },
  "scripts": {
    "dev": "NODE_ENV=development webpack-dev-server --host 0.0.0.0 --port 3024 --config webpack.dev.js --history-api-fallback --content-base example",
    "test": "jest",
    "test:ci": "jest --coverage && cat ./reports/lcov.info | ./node_modules/coveralls/bin/coveralls.js",
    "coverage": "jest --coverage",
    "build": "npm run clean && tsc && tsc -m es6 --outDir lib-esm && NODE_ENV=production webpack",
    "clean": "shx rm -rf lib lib-esm bundles",
    "prepublish": "npm run build"
  },
  "jest": {
    "transform": {
      "^.+\\.tsx?$": "<rootDir>/preprocessor.js"
    },
    "testRegex": "(/test/.*|\\.(test|spec))\\.(ts|tsx|js)$",
    "moduleFileExtensions": [
      "ts",
      "tsx",
      "js",
      "jsx"
    ],
    "mapCoverage": true,
    "coverageDirectory": "reports",
    "collectCoverageFrom": [
      "**/src/**/*.{ts,tsx}",
      "!**/src/**/index.ts"
    ]
  },
  "keywords": [
    "bulma",
    "react"
  ],
  "repository": {
    "type": "git",
    "url": "https://github.com/AlgusDark/bulma-jsx.git"
  },
  "bugs": {
    "url": "https://github.com/AlgusDark/bulma-jsx/issues"
  },
  "author": {
    "email": "algus.dark@gmail.com",
    "name": "Algus Dark",
    "url": "http://algus.ninja"
  },
  "license": "MIT"
}
