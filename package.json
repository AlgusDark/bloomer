{
  "name": "bloomer",
  "version": "0.7.0",
  "description": "A set of React Components for Bulma CSS",
  "typings": "dist/bloomer.d.ts",
  "main": "dist/bloomer.cjs.js",
  "jsnext:main": "dist/bloomer.es.js",
  "module": "dist/bloomer.es.js",
  "browser": {
    "./dist/bloomer.es.js": "./dist/bloomer.browser.es.js",
    "./dist/bloomer.cjs.js": "./dist/bloomer.browser.cjs.js"
  },
  "peerDependencies": {
    "react": ">= 0.14.0 < 17.0.0-0"
  },
  "devDependencies": {
    "@babel/standalone": "^7.0.0-beta.51",
    "@types/node": "^10.5.0",
    "@types/react": "^16.4.4",
    "@types/react-dom": "^16.0.6",
    "cross-env": "^5.2.0",
    "enzyme": "^3.3.0",
    "enzyme-adapter-react-16": "^1.1.1",
    "express": "^4.16.3",
    "husky": "^0.14.3",
    "jest": "^23.2.0",
    "lint-staged": "^7.2.0",
    "node-watch": "^0.5.8",
    "prettier": "^1.13.6",
    "react": "^16.4.1",
    "react-dom": "^16.4.1",
    "rimraf": "^2.6.2",
    "rollup": "^0.62.0",
    "rollup-plugin-commonjs": "^9.1.3",
    "rollup-plugin-ignore": "^1.0.3",
    "rollup-plugin-json": "^3.0.0",
    "rollup-plugin-node-resolve": "^3.3.0",
    "rollup-plugin-replace": "^2.0.0",
    "rollup-plugin-sourcemaps": "^0.4.2",
    "rollup-plugin-terser": "^1.0.1",
    "rollup-plugin-typescript2": "^0.15.1",
    "rollup-plugin-visualizer": "^0.8.0",
    "tslint": "^5.10.0",
    "tslint-config-prettier": "^1.13.0",
    "tslint-config-standard": "^7.1.0",
    "typescript": "^2.9.2"
  },
  "scripts": {
    "lint": "tslint --project ./tsconfig.json -t codeFrame '{src,test}/**/*.{ts, tsx}'",
    "prebuild": "rimraf dist",
    "build": "rollup -c",
    "test": "jest",
    "precommit": "lint-staged",
    "prepublishOnly": "npm run build",
    "dev": "cross-env node example/server.js"
  },
  "lint-staged": {
    "{src,test}/**/*.{ts, tsx}": [
      "prettier --write",
      "lint",
      "git add"
    ]
  },
  "keywords": [
    "bulma",
    "react",
    "styled-components"
  ],
  "repository": {
    "type": "git",
    "url": "https://github.com/AlgusDark/bloomer.git"
  },
  "bugs": {
    "url": "https://github.com/AlgusDark/bloomer/issues"
  },
  "author": {
    "email": "algus.dark@gmail.com",
    "name": "Algus Dark",
    "url": "http://bloomer.js.org"
  },
  "license": "MIT",
  "homepage": "http://bloomer.js.org",
  "dependencies": {
    "@types/acorn": "^4.0.3",
    "@types/chokidar": "^1.7.5",
    "magic-string": "^0.25.0",
    "source-map": "^0.7.3"
  }
}
