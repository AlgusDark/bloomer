{
  "name": "bulma-jsx",
  "version": "0.1.4",
  "description": "A set of React Components for Bulma CSS",
  "main": "lib/index.js",
  "types": "lib/index.d.ts",
  "jsnext:main": "lib-esm/index.js",
  "module": "lib-esm/index.js",
  "dependencies": {
    "classnames": "^2.2.5",
    "tslib": "^1.7.0"
  },
  "devDependencies": {
    "@types/classnames": "^0.0.32",
    "@types/enzyme": "^2.8.0",
    "@types/node": "^7.0.15",
    "@types/react": "^15.0.23",
    "@types/react-dom": "^15.5.0",
    "@types/tape": "^4.2.30",
    "awesome-typescript-loader": "^3.1.3",
    "bulma": "^0.4.1",
    "css-loader": "^0.28.1",
    "enzyme": "^2.8.2",
    "faucet": "^0.0.1",
    "nyc": "^10.3.2",
    "react": "^15.5.4",
    "react-dom": "^15.5.4",
    "react-test-renderer": "^15.5.4",
    "shx": "^0.2.2",
    "style-loader": "^0.17.0",
    "tape": "^4.6.3",
    "ts-node": "^3.0.4",
    "typescript": "^2.3.2",
    "webpack": "^2.4.1",
    "webpack-dev-server": "^2.4.5"
  },
  "scripts": {
    "dev": "NODE_ENV=development webpack-dev-server --host 0.0.0.0 --port 3024 --config webpack.dev.js --history-api-fallback --content-base example",
    "test": "ts-node node_modules/tape/bin/tape 'src/**/*.test.tsx' | faucet",
    "cover": "nyc node_modules/tape/bin/tape 'src/**/*.test.tsx'",
    "build": "npm run clean && tsc && tsc -m es6 --outDir lib-esm && NODE_ENV=production webpack",
    "clean": "shx rm -rf lib lib-esm bundles",
    "prepublish": "npm run build"
  },
  "keywords": [
    "bulma",
    "react"
  ],
  "repository": {
    "type": "git",
    "url": "https://github.com/AlgusDark/bulma-jsx.git"
  },
  "bugs": {
    "url": "https://github.com/AlgusDark/bulma-jsx/issues"
  },
  "author": {
    "email": "algus.dark@gmail.com",
    "name": "Algus Dark",
    "url": "http://algus.ninja"
  },
  "license": "MIT"
}
